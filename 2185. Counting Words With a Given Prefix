class Solution {
    public int prefixCount(String[] words, String pref) {
        int count=0;
        for(int i=0;i<words.length;i++){
            if(pref.length()<=words[i].length()) {
                if(pref.equalsIgnoreCase(words[i].substring(0,pref.length()))){
                    count++;
                }
            }
        }
        return count;
    }
}
class Solution {
    public int finalValueAfterOperations(String[] operations) {
        int x = 0;
        for(String i : operations) {
            if(i.contains("+")) {
                if(i.charAt(0) == 'x') x++;
                else ++x;
            }
            else{
                if(i.charAt(0) == 'x') x--;
                else --x;
            }
        }
        return x;
    }
}
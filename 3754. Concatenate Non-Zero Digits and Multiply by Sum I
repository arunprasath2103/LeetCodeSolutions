class Solution {
    public long sumAndMultiply(int n) {
        long num = n, rev = 0, sum = 0, rem = 0;
        for( ; num > 0; rev = (rev * 10) + (num % 10), num /= 10);
        while(rev > 0){
            rem = rev % 10;
            if(rem != 0) {
                num = (num * 10) + rem;
                sum += rem;
            }
            rev /= 10;
        }
        return num * sum;
    }
}
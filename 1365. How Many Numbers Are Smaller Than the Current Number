class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int len = nums.length;
        int[] result = new int[len];
        int curr, count;
        for(int i=0; i < len; i++){
            count = 0;
            curr = nums[i];
            for(int j=0; j < len; j++){
                if(nums[j] < curr) count++;
            }
            result[i] = count;
        }
        return result;
    }
}
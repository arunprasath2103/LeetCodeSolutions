class Solution {
    public int[] getFinalState(int[] nums, int k, int multiplier) {
        int small = 0;
        for(int i = 0; i < k; i++) {
            for(int j = nums.length-1; j >= 0; j--) {
                small = (nums[small] >= nums[j]) ? j : small;
            }
            nums[small] *= multiplier; 
        }
        return nums;
    }
}
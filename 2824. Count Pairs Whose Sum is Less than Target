class Solution {
    public int countPairs(List<Integer> nums, int target) {
        int count = 0, len = nums.size();
        for(int i = 0; i < len; i++){
            for(int j = len-1; j > i; j--){
                if(nums.get(i)+nums.get(j) < target) count++;
            }
        }
        return count;
    }
}